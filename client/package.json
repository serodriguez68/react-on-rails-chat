{
  "name": "react_on_rails_chat",
  "engines": {
    "node": "7.8.0",
    "npm": "4.4.4",
    "yarn": "0.22.0"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "test": "NODE_PATH=./app mocha --compilers js:babel-core/register --require ./app/libs/testHelper.js --require ./app/libs/testNullCompiler.js 'app/**/*.spec.@(js|jsx)'",
    "test:debug": "yarn run test -- --debug-brk",
    "start": "babel-node server-express.js",
    "build:production:client": "NODE_ENV=production webpack --config webpack.config.js",
    "build:production:server": "NODE_ENV=production webpack --config webpack.config.js",
    "build:client": "webpack --config webpack.config.js",
    "build:dev:client": "NODE_ENV=development webpack -w --config webpack.config.js",
    "build:dev:server": "NODE_ENV=development webpack -w --config webpack.config.js",
    "build:server": "webpack --config webpack.config.js",
    "build:test": "rm -rf ../public/webpack/test && NODE_ENV=test yarn run build:client && NODE_ENV=test yarn run build:server",
    "build:production": "rm -rf ../public/webpack/production && NODE_ENV=production webpack --config webpack.config.js",
    "hot-assets": "NODE_ENV=development babel-node server-rails-hot.js",
    "lint": "eslint --ext .js,.jsx ."
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "actioncable": "^5.1.1",
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "es5-shim": "^4.5.9",
    "expose-loader": "^0.7.3",
    "imports-loader": "^0.7.1",
    "js-yaml": "^3.8.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-on-rails": "8.0.3",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^6.7.0",
    "redux-promise": "^0.5.3",
    "webpack": "^2.3.3",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "devDependencies": {}
}
